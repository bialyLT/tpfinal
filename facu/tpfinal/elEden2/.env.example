# URL de conexión para Postgres (se genera automáticamente)
DATABASE_URL=postgresql://postgres:tu_contraseña_segura_aqui@postgres:5432/eleden

# ============================================
# MERCADOPAGO
MERCADOPAGO_PUBLIC_KEY=tu_public_key_de_mercadopago_aqui

# ============================================
# NGROK
NGROK_AUTHTOKEN=tu_token_de_ngrok_aqui
